{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": ".",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/gsbMonolith/Forms/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
    "build": {
        "content": [
            {
                "files": [ "**/*.md", "**/*.yml" ],
                "exclude": [ "_site/**" ]
            },
            {
                "files": [ "toc.yml" ]
            }
        ],
        "resource": [
            {
                "files": [ "images/**" ]
            }
        ],
        "output": "_site",
        "template": [ "default", "modern" ],
        "globalMetadata": {
            "_appName": "gsbMonolith",
            "_appTitle": "gsbMonolith",
            "_enableSearch": false,
            "_appHome": "index.md",
            "pdf": true
        }
    }
}